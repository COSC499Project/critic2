# Load the molecule from the cube file generated by QE
molecule ../data/hf6_rhoae.cube
load ../data/hf6_rhoae.cube

# Change the gradient path termination criterion to make the 
# paths go to the end of the molecular cell.
ode_mode gradeps 1d-80

# Plot the basins for H and F
basinplot cube 4 cp 1 verbose
basinplot cube 4 cp 2 verbose
